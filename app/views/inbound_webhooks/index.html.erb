<h2>All Customers</h2>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Points Balance</th>
      <th>Updated At</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.first_name %></td>
      <td><%= customer.last_name %></td>
      <td><%= customer.email %></td>
      <td><%= customer.points_balance %></td>
      <td><%= customer.updated_at %></td>
    </tr>
  <% end %>
</table>
